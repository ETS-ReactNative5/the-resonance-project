{
  "expo": {
    "name": "Echo.Church",
    "description": "Echo.Church App",
    "slug": "echo",
    "scheme": "echo",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
